{
	"name": "cucumber-json-formatter",
	"publisher": "xletohic",
	"displayName": "Cucumber JSON formatter",
	"description": "Automatically formats JSON within Cucumber .feature files while preserving existing indentation",
	"repository": "https://github.com/xletohic-thefork/cucumber-json-formatter.git",
	"version": "2.2.0",
	"engines": {
		"vscode": "^1.96.0"
	},
	"icon": "images/logo.png",
	"categories": [
		"Formatters",
		"Other"
	],
	"main": "./dist/extension.js",
	"activationEvents": [],
	"contributes": {
		"commands": [
			{
				"command": "extension.formatCucumberFeatureJSON",
				"title": "Cucumber: Format JSON in current file"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run build",
		"build": "rslib build",
		"dev": "rslib build --watch",
		"watch": "rslib build --watch",
		"test": "jest"
	},
	"devDependencies": {
		"@babel/preset-env": "^7.26.7",
		"@babel/preset-typescript": "^7.26.0",
		"@biomejs/biome": "1.9.4",
		"@rslib/core": "^0.4.0",
		"@types/jest": "^29.5.14",
		"@types/mocha": "^10.0.10",
		"@types/node": "^22.13.0",
		"@types/vscode": "^1.96.0",
		"babel-jest": "^29.7.0",
		"jest": "^29.7.0",
		"mocha": "^11.1.0",
		"tar": ">=4.4.2",
		"ts-jest": "^29.2.5",
		"typescript": "^5.7.3"
	},
	"dependencies": {
		"@vscode/vsce": "^3.2.2",
		"global": "^4.4.0",
		"indent-string": "^5.0.0"
	},
	"packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
